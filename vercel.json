{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "narduk-games-hub",
  "buildCommand": "echo 'Landing page ready'",
  "outputDirectory": ".",
  "installCommand": "echo 'No dependencies needed for landing page'",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ]
}

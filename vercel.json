{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "buildCommand": "npm run install:all && npm run build:all:subdir",
  "installCommand": "echo 'Dependencies installed in build'",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/lexistack/(.*)",
      "destination": "/games/lexi-stack/dist/$1",
      "has": [
        {
          "type": "host",
          "value": "games.nard.uk"
        }
      ]
    },
    {
      "source": "/lexistack",
      "destination": "/games/lexi-stack/dist/index.html",
      "has": [
        {
          "type": "host",
          "value": "games.nard.uk"
        }
      ]
    },
    {
      "source": "/wordle/(.*)",
      "destination": "/games/wordle-clone/dist/$1",
      "has": [
        {
          "type": "host",
          "value": "games.nard.uk"
        }
      ]
    },
    {
      "source": "/wordle",
      "destination": "/games/wordle-clone/dist/index.html",
      "has": [
        {
          "type": "host",
          "value": "games.nard.uk"
        }
      ]
    },
    {
      "source": "/dictionary.json",
      "destination": "/games/wordle-clone/dist/dictionary.json",
      "has": [
        {
          "type": "host",
          "value": "games.nard.uk"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/games/lexi-stack/dist/$1",
      "has": [
        {
          "type": "host",
          "value": "lexistack.nard.uk"
        }
      ]
    },
    {
      "source": "/",
      "destination": "/games/lexi-stack/dist/index.html",
      "has": [
        {
          "type": "host",
          "value": "lexistack.nard.uk"
        }
      ]
    },
    {
      "source": "/(.*)",
      "destination": "/games/wordle-clone/dist/wordle/$1",
      "has": [
        {
          "type": "host",
          "value": "wordle.nard.uk"
        }
      ]
    },
    {
      "source": "/",
      "destination": "/games/wordle-clone/dist/wordle/index.html",
      "has": [
        {
          "type": "host",
          "value": "wordle.nard.uk"
        }
      ]
    },
    {
      "source": "/",
      "destination": "/index.html",
      "has": [
        {
          "type": "host",
          "value": "games.nard.uk"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/games/lexi-stack/dist/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ],
      "has": [
        {
          "type": "host",
          "value": "lexistack.nard.uk"
        }
      ]
    },
    {
      "source": "/games/wordle-clone/dist/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ],
      "has": [
        {
          "type": "host",
          "value": "wordle.nard.uk"
        }
      ]
    }
  ]
}
